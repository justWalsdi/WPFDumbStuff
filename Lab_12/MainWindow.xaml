<Window x:Class="Lab_12.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab_12"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Извлечь" Click="Open" HorizontalAlignment="Left" Margin="241,10,0,0" VerticalAlignment="Top" Width="75"/>
        <DataGrid x:Name="dgGrid" AutoGenerateColumns="False" HorizontalAlignment="Left" Height="311" Margin="37,58,0,0" VerticalAlignment="Top" Width="704">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Сырье" Binding="{Binding Nam}"/>
                <DataGridTextColumn Header="СВ,%" Binding="{Binding Suh}"/>
                <DataGridTextColumn Header="Срок хранения, сут" Binding="{Binding Srok}"/>
            </DataGrid.Columns>
        </DataGrid>


    </Grid>
</Window>
